{
  "name": "trodat",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "yum-update": "npm install yup",
    "yum-install": "sudo yum install xorg-x11-server-Xvfb",
    "install-deps": "npm run yum-update && yum-install",
    "check-device": "node -e \"console.log('Platform: ' + process.platform + ', Architecture: ' + process.arch)\"",
    "check-xauth": "echo 'To install xauth, run the following command based on your OS:' && echo 'For Ubuntu/Debian: sudo apt-get install -y xauth' && echo 'For CentOS/RHEL: sudo yum install -y xorg-x11-xauth' && echo 'For Alpine: apk add --no-cache xauth'",
    "install-build": "xvfb-run -a nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "command:init": "ts-node src/console init",
    "playwright:install": "npx playwright install",
    "playwright:install-deps": "if [ -x \"$(command -v apt-get)\" ]; then sudo apt-get install -y wget; elif [ -x \"$(command -v apk)\" ]; then apk add --no-cache wget; elif [ -x \"$(command -v yum)\" ]; then sudo yum install -y wget; else echo 'Unsupported OS'; exit 1; fi && npx playwright install-deps",
    "install-start": "xvfb-run -a nest start",
    "start": "npm run playwright:install-deps && npm run playwright:install && npm run install-deps && npm run install-start",
    "start:dev": "nest start --watch"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/mongoose": "^10.0.6",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/typeorm": "^10.0.2",
    "antd": "^5.20.3",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "firebase-admin": "^12.5.0",
    "ioredis": "^5.4.1",
    "mongodb": "^5.9.2",
    "mongoose": "^8.4.0",
